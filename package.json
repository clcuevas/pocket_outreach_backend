{
  "name": "pocket_outreach_backend",
  "version": "0.0.1",
  "description": "backend server for Pocket Outreach a simple web app designed to connect homeless people to resources near them in the Seattle area",
  "main": "server.js",
  "dependencies": {
    "cli-color": "^1.2.0",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "moment": "^2.18.1",
    "superagent": "^3.5.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "nock": "^9.0.11",
    "nodemon": "^1.11.0",
    "sinon": "^2.1.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha --recursive api/",
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "debug": "node $NODE_DEBUG_OPTION server.js",
    "lint": "./node_modules/.bin/eslint server.js && ./node_modules/.bin/eslint ./api/*.js",
    "docs": "./node_modules/.bin/rimraf doc/ out/ && ./node_modules/.bin/apidoc -i api/ -o doc/ && ./node_modules/.bin/jsdoc api/ -r README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clcuevas/pocket_outreach_backend.git"
  },
  "author": "Michae Bowen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clcuevas/pocket_outreach_backend/issues"
  },
  "homepage": "https://github.com/clcuevas/pocket_outreach_backend#readme",
  "apidoc": {
    "title": "Pocket Outreach API",
    "version": "0.0.1",
    "description": "backend server for Pocket Outreach a simple web app designed to connect homeless people to resources near them in the Seattle area"
  }
}
