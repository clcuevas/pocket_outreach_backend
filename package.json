{
  "name": "pocket_outreach_backend",
  "version": "0.3.0",
  "description": "This is the REST API for Pocket Outreach, a web app designed to connect people in need with resources in their area. The areas we currently cover are: Seattle",
  "main": "server.js",
  "dependencies": {
    "cli-color": "^1.2.0",
    "compression": "^1.6.2",
    "config": "^1.25.1",
    "dotenv": "^4.0.0",
    "dotenv-safe": "^4.0.4",
    "express": "^4.15.2",
    "moment": "^2.18.1",
    "mongodb": "^2.2.25",
    "mongoose": "^4.9.4",
    "superagent": "^3.5.2",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.1",
    "@types/cli-color": "^0.3.29",
    "@types/mocha": "^2.2.41",
    "@types/nock": "^8.2.1",
    "@types/sinon": "^2.2.1",
    "@types/winston": "^2.3.2",
    "apidoc": "^0.17.5",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.1.1",
    "jsdoc": "^3.4.3",
    "mocha": "^3.2.0",
    "mockgoose": "^7.1.1",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.0",
    "proxyquire": "^1.7.11",
    "sinon": "^2.1.0"
  },
  "scripts": {
    "test": "nyc ./node_modules/.bin/mocha --recursive api/**/*.test.js lib/**/*.test.js && nyc report --reporter=text-lcov | coveralls -service = travis-ci",
    "start": "NODE_ENV=production node server.js | ./node_modules/.bin/mongod --dbpath=./data",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dev": "NODE_ENV=development ./node_modules/.bin/nodemon server.js | ./node_modules/.bin/mongod --dbpath=./data",
    "debug": "node $NODE_DEBUG_OPTION server.js",
    "start-mongo": "./node_modules/.bin/mongod --dbpath=./data",
    "start-server": "NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "lint": "./node_modules/.bin/eslint server.js && ./node_modules/.bin/eslint ./api/*.js",
    "docs": "./node_modules/.bin/rimraf doc/ out/ && ./node_modules/.bin/apidoc -i api/ -o doc/ && ./node_modules/.bin/jsdoc api/ lib/ -r README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clcuevas/pocket_outreach_backend.git"
  },
  "author": "Michael Bowen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clcuevas/pocket_outreach_backend/issues"
  },
  "homepage": "https://github.com/clcuevas/pocket_outreach_backend#readme",
  "apidoc": {
    "title": "Pocket Outreach API",
    "version": "0.0.1",
    "description": "backend server for Pocket Outreach a simple web app designed to connect homeless people to resources near them in the Seattle area"
  }
}
